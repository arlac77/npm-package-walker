{
  "name": "npm-package-walker",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/package-walker.js",
  "module": "src/package-walker.js",
  "description": "Walks down the local npm package dependency tree and calls a visitor function for each package",
  "keywords": [
    "dependecy"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "test": "ava",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "documentation readme src/package-walker.js --section=API",
    "semantic-release": "semantic-release",
    "lint": "documentation lint src/package-walker.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^1.0.0-rc.2",
    "documentation": "^8.1.2",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.67.3",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "semantic-release": "^15.12.2",
    "travis-deploy-once": "^5.0.9",
    "c8": "^3.2.1",
    "esm": "^3.0.84"
  },
  "engines": {
    "node": ">=10.13"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-package-walker.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/npm-package-walker/issues"
  },
  "homepage": "https://github.com/arlac77/npm-package-walker#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    },
    "files": [
      {
        "merger": "Readme",
        "options": {
          "badges": {
            "Maintainability": {
              "id": "15cd579a3cc8090fb1d7"
            }
          }
        }
      }
    ]
  }
}
