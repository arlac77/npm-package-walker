{
  "name": "npm-package-walker",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/package-walker.mjs",
  "exports": {
    ".": "./src/package-walker.mjs"
  },
  "description": "Walks down the local npm package dependency tree and calls a visitor function for each package",
  "keywords": [
    "dependecy"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/package-walker.mjs --section=API && documentation readme ./src/package-walker.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint src/package-walker.mjs && documentation lint ./src/package-walker.mjs"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.5",
    "documentation": "^13.1.0",
    "semantic-release": "^17.2.2"
  },
  "engines": {
    "node": ">=12.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/npm-package-walker.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/npm-package-walker/issues"
  },
  "homepage": "https://github.com/arlac77/npm-package-walker#readme",
  "template": {
    "mergers": [
      {
        "type": "Readme",
        "options": {
          "badges": [
            {
              "name": "Maintainability",
              "id": "15cd579a3cc8090fb1d7"
            }
          ]
        }
      }
    ],
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-github"
    ]
  }
}
